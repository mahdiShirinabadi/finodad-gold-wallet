					"if (processRequests.includes(pm.info.requestName)) {",
					"    const uuid = pm.collectionVariables.get('cashInUUID') || pm.collectionVariables.get('cashOutUUID') || pm.collectionVariables.get('p2pUUID') || pm.collectionVariables.get('buyUUID') || pm.collectionVariables.get('sellUUID') || pm.collectionVariables.get('giftCardUUID');",
					"    const amount = pm.collectionVariables.get('cashInAmount') || pm.collectionVariables.get('cashOutAmount') || pm.collectionVariables.get('p2pAmount') || pm.collectionVariables.get('buyQuantity') || pm.collectionVariables.get('sellQuantity') || pm.collectionVariables.get('giftCardQuantity');",
					"    const nationalCode = pm.collectionVariables.get('nationalCode');",
					"    ",
					"    if (uuid && amount && nationalCode) {",
					"        const signData = `${uuid}|${amount}|${nationalCode}`;",
					"        const generatedSign = btoa(signData); // Base64 encode for demo",
					"        pm.collectionVariables.set('generatedSign', generatedSign);",
					"    }",
					"}"
				]
			}
